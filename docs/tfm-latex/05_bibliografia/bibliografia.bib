@Misc{TFGLuis,
	language = {español},
	author = {Cruz Ramos, Luis Fernando and Parapar, Javier},
	abstract = {Esta propuesta de aplicación web trata de resolver la saturación de información para el colectivo de los músicos. Consiste en una plataforma similar a una red social para músicos que deseen comentar, anunciar y discutir eventos musicales interesantes en su zona. Asimismo, la aplicación también permitirá realizar anuncios como una búsqueda de músicos para tocar en una determinada ocasión (o para formar un grupo, etc.) o publicar anuncios de venta de instrumentos, entre otros. También tendrá un tipo de elemento para discutir temas de actualidad musical en la aplicación. Además, la web servirá de repositorio privado de las composiciones (en formato PDF) de los músicos para que puedan compartirlas públicamente con sus seguidores, Y así recibir retroalimentación y poder mejorar sus composiciones. Por otro lado, los usuarios pueden seleccionar categorías de interés para filtrar sus búsquedas. Todos los tipos de publicaciones descritas tendrán secciones para ser comentadas y podrán recomendarse a los seguidores de los usuarios. Los usuarios serán notificados de las novedades en sus publicaciones o actualizaciones de los usuarios que sigan a través de un sistema de notificaciones. La gestión de las publicaciones, recomendaciones y comentarios será realizada únicamente por su respectivo autor. Esta propuesta de aplicación será llevada a cabo utilizando tecnologías software actuales, como Spring Boot y Angular, además de otras herramientas para la gestión del desarrollo del software (como GitLab, Taiga, Jenkins y Sonar). Asimismo, la aplicación será desarrollada utilizando conjuntamente las metodologías ágiles Scrum y Extreme Programming. Palabras clave: Angular, Bootstrap, Extreme Programming, Git, GitLab, Hibernate, HTML, Java, Jenkins, JPA, Maven, MVC, MySQL, NodeJS, Scrum, Spring Boot, Sonar, Taiga, UML, Visual Studio Code, XP},
	keywords = {Sitios Web -- Desarrollo -- Trabajos fin de grado},
	language = {spa},
	publisher = {Traballo fin de grao-Universidade da Coruña},
	series = {Traballos fin de grao (UDC. INF)},
	title = {Aplicación web dedicada a la gestión de partituras y comunicación de eventos y anuncios musicales},
	year = {2019},
}

